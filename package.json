{
  "name": "main",
  "version": "1.0.0",
  "description": "The main Harmelodic website",
  "main": "public/index.html",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src/* --fix",
    "start": "webpack serve --config webpack.dev.js",
    "start:prod": "webpack serve --config webpack.prod.js",
    "build": "webpack --config webpack.prod.js",
    "docker:build": "docker build -t $npm_package_name:latest .",
    "docker:run": "docker run --name $npm_package_name -d -p 8080:80 $npm_package_name:latest",
    "docker": "npm run build && npm run docker:build && npm run docker:run",
    "docker:destroy": "docker stop $npm_package_name && docker rm $npm_package_name && docker rmi $npm_package_name"
  },
  "author": "Harmelodic",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-react": "^7.12.10",
    "babel-loader": "^8.2.2",
    "compression-webpack-plugin": "^7.1.1",
    "eslint": "^7.16.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-react": "^7.22.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.2.1",
    "webpack": "^5.11.1",
    "webpack-cli": "^4.3.0",
    "webpack-dev-server": "^3.11.1",
    "webpack-merge": "^5.7.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/Harmelodic/main.git"
  },
  "bugs": {
    "url": "https://gitlab.com/Harmelodic/main/issues"
  },
  "homepage": "https://gitlab.com/Harmelodic/main#README",
  "keywords": [
    "frontend",
    "react",
    "harmelodic",
    "boilerplate",
    "template",
    "redux"
  ],
  "license": "UNLICENSED"
}
